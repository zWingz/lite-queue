{
  "extends": "tslint-config-standard",
  "rules": {
    "no-floating-promises": false,
    "space-before-function-paren": false,
    "whitespace": [
      true,
      "check-decl",
      "check-operator",
      "check-module",
      "check-separator",
      "check-rest-spread",
      "check-type",
      "check-typecast",
      "check-type-operator",
      "check-preblock",
      "check-postbrace"
    ]
  }
}
